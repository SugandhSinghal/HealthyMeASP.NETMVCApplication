@model IEnumerable<HealthyMe.Models.Appointment>

@{
    ViewBag.Title = "Calendar";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
@*This is for calender*@
<h2>Calendar</h2>
<div id="calendar"></div>
  <div style="display: none">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ClientName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrainerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AppointmentAddress)
            </th>
            <th></th>
        </tr>

      @foreach (var item in Model)
            {
                <tr class="events">
                    <td class="title">
                        @Html.DisplayFor(modelItem => item.ClientName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TrainerName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Date)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AppointmentAddress)
                    </td>
                    
                </tr>
            }
        </table>
     </div>
    @section Scripts {
        @Scripts.Render("~/bundles/fullcalendar")
    }



   